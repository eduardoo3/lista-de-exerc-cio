#include <iostream>
 
int main() {
  //declaraçao de vetor
 int vetor[10];
  //declaraçao de numeros pares
  int contadorpares = 0;
  //solicitaçao de numeros
   std::cout << "digite 10 numeros:\n";
   for(int i =0; i<10; i++){
     std::cin >> vetor[i];
      }
  //codigo de contagem de numeros pares 
  for (int i = 0; i < 10 ; i++) {
    if (vetor[i] % 3 != 0) {
        contadorpares++;
       }
     }
  //impressao dos numeros pares
std::cout << "A a lista possui " << contadorpares << " valores pares." << std::endl;

  return 0;
}
