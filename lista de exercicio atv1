
int main() {
    int A[6] = {1,0,5,-2,-5,7};
    
    int soma = A[0] + A[1] + A[5];
    std::cout << "A soma dos elementos A[0], A[1] e A[5] é: " << soma << std::endl;
    A[4] = 100;
    std::cout << "os valores do vetor A são:" << std::endl;
     for (int i = 0; i < 6; i++) {
         std::cout << A[i] << std::endl;
        }
     return 0;
}
