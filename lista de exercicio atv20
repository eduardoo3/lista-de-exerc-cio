#include <iostream>
using namespace std;

int main() {
    // Declaração do vetor de 50 posições
    int vetor[50];

    // Preenchimento do vetor com a fórmula (i + 5 * i) % (i + 1)
    for (int i = 0; i < 50; ++i) {
        vetor[i] = (i + 5 * i) % (i + 1);
    }

    // Impressão do vetor na tela
    cout << "Vetor preenchido:" << endl;
    for (int i = 0; i < 50; ++i) {
        cout << vetor[i] << " ";
    }
    cout << endl;

    return 0;
}
